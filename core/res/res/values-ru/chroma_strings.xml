    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_accessTorchService">доступ к сервисам включения</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessTorchService">Предоставляет доступ к службе включения. Никогда не должна быть необходима для нормаьных приложений.</string>

    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_security">Безопасность</string>
    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_security">Разрешения, связанные с информацией о безопасности устройства.</string>

    <!-- [CHAR LIMIT=NONE] Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_readPhoneBlacklist">чтение чёрного списка</string>
    <!-- [CHAR LIMIT=NONE] Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_readPhoneBlacklist">Позволяет приложению считывать информацию о телефонных номерах, заблокированных для входящих вызовов или сообщений.</string>

    <!-- [CHAR LIMIT=NONE] Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_changePhoneBlacklist">изменение чёрного списка</string>
    <!-- [CHAR LIMIT=NONE] Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changePhoneBlacklist">Позволяет приложениям изменять телефонные номера, которые заблокированы для входящих вызовов или сообщений.</string>

    <!-- Notification which notifies user flashlight is enabled -->
    <string name="quick_settings_tile_flashlight_not_title">Включить фонарик</string>
    <string name="quick_settings_tile_flashlight_not_summary">Нажмите, чтобы выключить</string>

    <string name="silent_mode_priority">Важные</string>
    <string name="silent_mode_none">Ничего</string>


    <!-- Privacy Guard -->
    <string name="permlab_changePrivacyGuardState">Включить защищённый режим</string>
    <string name="permdesc_changePrivacyGuardState">Выберите приложения для работы с защищённым режимом. Когда приложение работает в защищённом режиме, оно не будет иметь доступа к персональным данным, таким как контакты, журналы вызовов, или сообщений.</string>
    <string name="privacy_guard_notification">Защищённый режим активен</string>
    <string name="privacy_guard_notification_detail"><xliff:g id="app">%1$s</xliff:g> не сможет получить доступ к личной информации</string>
    <string name="privacy_guard_custom_notification_detail"><xliff:g id="app">%1$s</xliff:g> не сможет получить доступ к пользовательским разрешениям</string>

    <!-- label for current user in phone options dialog -->
    <string name="global_action_current_user">Текущее</string>
